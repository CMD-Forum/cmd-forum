    "studio": "dotenv -e .env -- npx prisma studio",
    "studio:dev": "dotenv -e .env.local -- npx prisma studio",
    "postinstall": "npx prisma generate",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
